<div>
  <table id="<%=  dasherize(title) %>_table">
    <h3><%= title %></h3>

    <tr>
      <th scope="col"><%= type %></th>
      <th scope="col"><%= column_name(title) %></th>
    </tr>
    <% highest_first(data).each do |line| %>
      <tr>
        <th scope="row"><%= line[0] %></th>
        <td class="<%= dasherize(line[0]) %>_<%= dasherize(title)%>"><%= line[1] %></td>
      </tr>
    <%end%>
  </table>
</div>
